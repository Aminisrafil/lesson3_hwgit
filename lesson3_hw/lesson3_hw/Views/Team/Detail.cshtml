@model Team
@{
    ViewData["Title"] = "Detail";
}

 <div class="col-lg-3 col-md-6 d-flex align-items-stretch m-auto mt-5" data-aos="fade-up" data-aos-delay="100">
                <div class="member">
                  <div class="member-img">
                    <img src="~/assets/img/team/team-@(Model.Id).jpg" class="img-fluid" alt="">
                    <div class="social">
                      <a href=""><i class="bi bi-twitter"></i></a>
                      <a href=""><i class="bi bi-facebook"></i></a>
                      <a href=""><i class="bi bi-instagram"></i></a>
                      <a href=""><i class="bi bi-linkedin"></i></a>
                    </div>
                  </div>
                  <div class="member-info">
                        <h4>@Model.Name </h4>
                    <span>@Model.Occupation</span>
                  </div>
                </div>
 </div>

